@model List<string>

<!-- slider -->
<div class="slider-layer">
    <div id="slider-one" class="owl-carousel">
        @foreach (var item in Model)
        {
            <div class="item">
                <a href="#"><img src="/Images/@item" alt="@item" style="width: 100%;height: 282px"></a>
            </div>
        }
    </div>
</div> 
<!-- End slider -->